{
    "name": "Confluence Section Link",
    "manifest_version": 2,
    "version": "0.1.2",
    "description": "Make internal link of the section in confluence.",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["sectionlink.js"]
        }
    ],
    "permissions": ["storage"],
    "options_page": "options.html",
    "web_accessible_resources": ["link.png"]
}
