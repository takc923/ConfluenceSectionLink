{
    "name": "Confluence Section Link",
    "manifest_version": 2,
    "version": "0.0.1",
    "description": "コンフルのページ内リンクを作る",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["sectionlink.js"]
        }
    ],
    "permissions": ["storage"],
    "options_page": "options.html",
    "web_accessible_resources": ["link.png"]
}
